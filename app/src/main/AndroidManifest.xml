<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.ncfrcteams.frcscoutinghub2016">

    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/frc_scout_2016_icon_resized"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".ui.plana.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.plana.hubselection.HubListActivity"
            android:label="@string/title_activity_hub_list"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.plana.hub.HubSetupActivity"
            android:label="@string/title_activity_hub_setup"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.plana.hub.HubInfoPage"
            android:label="@string/title_activity_hub_info_page"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.plana.FragmentPagerActivity" />
        <activity
            android:name=".planb.MainActivityB"
            android:label="@string/title_activity_main_activity_b"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".planb.ScheduleSelectActivity"
            android:label="@string/title_activity_schedule_select"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
    </application>

</manifest>
